{
  "name": "openapi-ts-sdk-builder",
  "version": "4.0.0",
  "description": "Create a TypeScript SDK from an OpenAPI 3 definition",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "architecture": "jsarch src/*.ts > ARCHITECTURE.md && git add ARCHITECTURE.md",
    "build": "rimraf -f 'dist' && tsc --outDir dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cover": "npm run jest -- --coverage",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "doc": "echo \"# API\" > API.md; jsdoc2md dist/*.js >> API.md && git add API.md",
    "jest": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest",
    "lint": "eslint src/*.ts",
    "metapak": "metapak",
    "precz": "npm run build && npm t && npm run lint && npm run architecture && npm run doc && npm run metapak -- -s",
    "prettier": "prettier --write src/*.ts",
    "preversion": "npm run build && npm t && npm run lint && npm run architecture && npm run doc && npm run metapak -- -s",
    "test": "npm run jest",
    "version": "npm run changelog"
  },
  "metapak": {
    "configs": [
      "main",
      "tsesm",
      "jest",
      "readme",
      "eslint",
      "coveralls",
      "ghactions",
      "jsarch",
      "jsdocs"
    ],
    "data": {
      "files": "src/*.ts",
      "eslintConfigType": "backend",
      "testFiles": "src/*.test.ts",
      "distFiles": "dist/*.js",
      "ignore": [
        "dist"
      ],
      "jestRoots": [
        "<rootDir>/src"
      ],
      "bundleFiles": [
        "dist",
        "src"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfroidure/openapi-ts-sdk-builder.git"
  },
  "keywords": [
    "openapi",
    "openapi3",
    "sdk",
    "client",
    "typescript"
  ],
  "author": {
    "name": "Nicolas Froidure",
    "email": "nicolas.froidure@insertafter.com",
    "url": "https://insertafter.com/en/index.html"
  },
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "camelcase": "^7.0.0",
    "schema2dts": "^4.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "coveralls": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.15.6",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.1",
    "jsarch": "^6.0.0",
    "jsdoc-to-markdown": "^7.1.1",
    "metapak": "^4.0.4",
    "metapak-nfroidure": "12.3.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/openapi-ts-sdk-builder/issues"
  },
  "homepage": "https://github.com/nfroidure/openapi-ts-sdk-builder#readme",
  "engines": {
    "node": ">=16.15.0"
  },
  "contributors": [],
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli",
      "typescript",
      "rimraf",
      "jest",
      "coveralls",
      "eslint",
      "eslint-config-prettier",
      "eslint-plugin-prettier",
      "prettier",
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "jsarch",
      "jsdoc-to-markdown"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        {
          "sourcemap": true,
          "format": "esm"
        }
      ]
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "script",
      "modules": true
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "parser": "@typescript-eslint/parser",
    "ignorePatterns": [
      "*.d.ts"
    ]
  },
  "prettier": {
    "semi": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "babel": {
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "node": "12.19.0"
          }
        }
      ]
    ],
    "env": {
      "cjs": {
        "presets": [
          [
            "@babel/env",
            {
              "targets": {
                "node": "10"
              },
              "modules": "commonjs"
            }
          ]
        ],
        "comments": true
      },
      "mjs": {
        "presets": [
          [
            "@babel/env",
            {
              "targets": {
                "node": "12"
              },
              "modules": false
            }
          ]
        ],
        "comments": false
      }
    },
    "sourceMaps": true
  },
  "jsarch": {
    "parserOptions": {
      "plugins": [
        "typescript"
      ]
    }
  },
  "type": "module"
}